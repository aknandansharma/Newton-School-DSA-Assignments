/*  
    
class Node {
    Node next;
    int val;

    Node(int val) {
        this.val = val;
        next = null;
       
    }
}
 */

Node top = null;
public void push(int x)
{
	Node temp = new Node(x);
	temp.next = top;
	top = temp;
}

public void pop()
{

	if (top == null) {
	}
	else {
	top = (top).next;}
}

public void top()
{
			// check for stack underflow
			if (top == null) {
			System.out.println("0");
			}
			else {
			Node temp = top;
			System.out.println(temp.val);
			}
}
