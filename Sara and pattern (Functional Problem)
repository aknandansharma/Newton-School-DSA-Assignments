static void Pattern(int N){
//Enter your code here
int p=0;
for(int i=0;i<N;){
    
    for(int j=0;j<N;j++){
        System.out.print(p+" ");
        p=p+4;
    }
    i++;
   p=6*i;
    System.out.println();
}



// 2nd method 

static void Pattern(int N){

//Enter your code here

int x=0;

for(int i=0;i<N;i++){

    for(int j=0;j<N;j++){

    System.out.print(x+ (4*j) +" ");

    }

System.out.println();

x+=6;

}

}
